{
  "name": "sams_portfolio",
  "version": "1.0.0",
  "description": "Sam's portfolio site for web development.",
  "main": "index.html",
  "scripts": {
    "copyfonts": "copyfiles -f node_modules/font-awesome/fonts/* dist/fonts",
    "dev": "lite-server",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'"
  },
  "repository": {
    "type": "git"
  },
  "author": "Samantha Tarrice",
  "license": "ISC",
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "lite-server": "^2.6.1",
    "node-sass": "^7.0.1",
    "onchange": "^7.1.0",
    "parallelshell": "3.0.2"
  },
  "dependencies": {
    "bootstrap": "^5.1.3",
    "font-awesome": "^4.7.0",
    "jquery": "^3.6.0",
    "popper.js": "^1.16.1"
  }
}
